{
  "name": "student_app_react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npm run test:unit",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watchAll --coverage",
    "test:unit:debug": "jest --debug",
    "test:unit:coverage": "jest --coverage",
    "prepare": "husky",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:verify": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
